{
  "name": "german-typing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "25.2.2",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json,css,scss,md,yml,yaml}": [
      "biome check --write"
    ]
  }
}

{
  "name": "german-typing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx,json,css,scss,md,yml,yaml}": [
      "biome check --write"
    ]
  }
}
